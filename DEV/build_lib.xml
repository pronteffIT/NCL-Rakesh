  	<!--Moving UAT Bar file to PROD folder for deployment-->
  	
  	<move file="${app.name}project.generated.bar" overwrite="true" tofile="${var.path}UAT/${app.name}project.generated.bar"/>
  	
  	 </target>
</project>
