<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
	attributeFormDefault="unqualified">
	<xs:include schemaLocation="BaseTypes.xsd"/>
	<xs:include schemaLocation="Agency.xsd"/>
	<xs:include schemaLocation="Client.xsd"/>
	<xs:include schemaLocation="ResShell.xsd"/>
	<xs:include schemaLocation="CouponShell.xsd"/>
	<xs:include schemaLocation="GroupShell.xsd"/>
	<xs:include schemaLocation="MsgSession.xsd"/>
	<xs:simpleType name="TemplateName">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EngineType">
		<xs:annotation>
			<xs:documentation>Document creation engine</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
			<xs:whiteSpace value="collapse"/>
			<xs:enumeration value="XSL_TRANSFORM"/>
			<xs:enumeration value="REPORTING_SERVICES"/>
			<xs:enumeration value="CRYSTAL"/>
			<xs:enumeration value="XSL_EXTERNAL"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ParameterType">
		<xs:annotation>
			<xs:documentation>Parameter type</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
			<xs:whiteSpace value="collapse"/>
			<xs:enumeration value="INTEGER"/>
			<xs:enumeration value="STRING"/>
			<xs:enumeration value="BOOLEAN"/>
			<xs:enumeration value="FLOAT"/>
			<xs:enumeration value="DATE"/>
			<xs:enumeration value="TIME"/>
			<xs:enumeration value="DATETIME"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DocBody">
		<xs:annotation>
			<xs:documentation>Encoded document body.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:base64Binary">
				<xs:attribute name="Binary" type="Boolean" default="N">
					<xs:annotation>
						<xs:documentation>If 'Y' - document body was encoded as-is, in other words, data stream produced by the reporting engine was just base64 encoded. Otherwise, document body was recognized as a string, converted to UTF8 encoding and then base64 encoded.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="DocBusinessArea">
		<xs:annotation>
			<xs:documentation>Business Area for the document function. May affect list of available documents and their format. Possible values are stored in the DOC_BIZ_AREA table.
</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DocRecipientInt">
		<xs:annotation>
			<xs:documentation>Specifies internal representation of recipient of documents</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="ClientID" type="RecordID" nillable="true">
				<xs:annotation>
					<xs:documentation>Client ID in case of Recipient is Client</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgentID" type="RecordID" nillable="true">
				<xs:annotation>
					<xs:documentation>Agent ID in case of Recipient is Agent</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgencyID" type="RecordID" nillable="true">
				<xs:annotation>
					<xs:documentation>Agency ID in case of Recipient is Agency</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HotelCode" type="HotelCode" nillable="true">
				<xs:annotation>
					<xs:documentation>Hotel Code in case of Recipient is Hotel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AltResRecipient" type="RecordID">
				<xs:annotation>
					<xs:documentation>Alternative agency recipient defined for the given reservation</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="DocRecipient">
		<xs:annotation>
			<xs:documentation>Specifies recipient of documents</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="ClientID" type="RecordID" nillable="true">
				<xs:annotation>
					<xs:documentation>Client ID in case of Recipient is Client</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgentID" type="RecordID" nillable="true">
				<xs:annotation>
					<xs:documentation>Agent ID in case of Recipient is Agent</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgencyID" type="RecordID" nillable="true">
				<xs:annotation>
					<xs:documentation>Agency ID in case of Recipient is Agency</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="HotelCode" type="HotelCode" nillable="true">
				<xs:annotation>
					<xs:documentation>Hotel Code in case of Recipient is Hotel</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="AltRecipient">
				<xs:annotation>
					<xs:documentation>Alternative Agency or Client Recipient</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
						<xs:whiteSpace value="collapse"/>
						<xs:enumeration value="CLIENT"/>
						<xs:enumeration value="AGENCY"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="ExpandableString">
		<xs:annotation>
			<xs:documentation>Defines a way of transformation or macro-substitution an input XML to provide a result value</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="MacroString" type="LongComment">
				<xs:annotation>
					<xs:documentation>String with macros in the form of XPATH expressions, for example:
'Dear &amp;//Client[1]/Salutation&amp;. &amp;//Client[1]/LastName&amp;. ....'
XPATH expressions assume that the source XML conforms to the schema
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MacroStringPointer" type="LongComment">
				<xs:annotation>
					<xs:documentation>Format is the same as MacroString but result is the name of transformation control XML (XSLBatchTransform) in Doc.Storage</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Transform" type="XSLBatchTransform">
				<xs:annotation>
					<xs:documentation>Transformation control XML</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocStorage" type="DocCode">
				<xs:annotation>
					<xs:documentation>Name of transformation control XML (XSLBatchTransform) in Doc.Storage</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:simpleType name="DocScheduleContext">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BusinessAreaInfo">
		<xs:annotation>
			<xs:documentation>Current business area with its attributes.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="Name" type="DocBusinessArea">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Attributes" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Attribute" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:all>
									<xs:element name="Name" type="xs:string">
										<xs:annotation>
											<xs:documentation>Parameter Name</xs:documentation>
											<xs:appinfo source="Versonix">
												<XMLBridge>Implemented</XMLBridge>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="Type" type="ParameterType" default="STRING"
										minOccurs="0">
										<xs:annotation>
											<xs:documentation>Type of Parameter Value</xs:documentation>
											<xs:appinfo source="Versonix">
												<XMLBridge>ToBeImplemented</XMLBridge>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="Value" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Parameter Value</xs:documentation>
											<xs:appinfo source="Versonix">
												<XMLBridge>Implemented</XMLBridge>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:all>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!--============================================= Input types-->
	<xs:complexType name="DocEntityRef">
		<xs:annotation>
			<xs:documentation>Entity choice that is used to produce DocEntityShell</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="ResID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="GroupID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CollectionID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CouponID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClientID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgencyID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmenityOrderID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="InvoiceID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Not Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="DocEntityShell">
		<xs:annotation>
			<xs:documentation>Entity Shell that can be used for applicability template calls</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="ResShell" type="ResShell">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="GroupShell" type="GroupShell"/>
			<xs:element name="CouponShell" type="CouponShell"/>
			<xs:element name="ClientShell" type="Client"/>
			<xs:element name="AgencyShell" type="Agency"/>
			<xs:element name="Transaction" type="AccTransaction">
				<xs:annotation>
					<xs:documentation>Accounting Transaction Shell</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="DocFile">
		<xs:annotation>
			<xs:documentation>Result of IssueDocs for FILE delivery. Can be used as input to delivery previously generated files</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Body" type="DocBody" minOccurs="0"/>
			<xs:element name="FileName" type="DocFileName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If body is provided, contains an original file name; otherwise contains a reference to a real file</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RenderFormat" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="RenderingFormat"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Extension" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used to provide a real file extension for UNK rendering format</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:whiteSpace value="collapse"/>
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:sequence minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provided to be put into DOC_TEMP_FILE_ tables</xs:documentation>
				</xs:annotation>
				<xs:element name="DocCode" type="DocCode"/>
				<xs:element name="KeyString" type="LongComment"/>
				<xs:element name="DataString" type="LongComment"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<!--============================================= Transformation output types-->
	<xs:complexType name="DocInstanceParameter">
		<xs:annotation>
			<xs:documentation>Real parameters values or link to set of possible values provided by applicability templates for instances</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code" type="xs:string"/>
			<xs:element name="Type" type="ParameterType" default="STRING" minOccurs="0"/>
			<xs:choice>
				<xs:element name="Value">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="UserInput">
					<xs:complexType>
						<xs:sequence>
							<xs:choice minOccurs="0">
								<xs:element name="DomainCode" type="LongCode">
									<xs:annotation>
										<xs:documentation>used to provide lookup ability and validate user input.
It refers to Seaware domains but there can also be built-in additional domains,
 like ALL_RES_AGENCIES, ALL_RES_HOTELS, etc.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="ValidValues" type="ExpandableString">
									<xs:annotation>
										<xs:documentation>String that contains a list of possible values (delimited by ";")</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
							<xs:element name="IsMandatory" type="Boolean"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="Comments" type="ShortComment" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DocInstanceSortAttr">
		<xs:annotation>
			<xs:documentation>Information to be used for sorting/grouping instances on UI</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="Code"/>
			<xs:element name="Value" type="ShortComment"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DocInstance">
		<xs:annotation>
			<xs:documentation>Document's occurrence with its identification (KeyString and DataString) and parameters</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="DocCode" type="DocCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Mandatory for attachments</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="KeyString" type="LongComment" minOccurs="0">
				<xs:annotation>
					<xs:documentation>used to identify document instance in the printing history, for example, for document code 'HOTEL VOUCHER' we will need hotel code, dates, client or guest ID to be part of the key string in order for the system to be able to show
whether this document was printed or not.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="DataString" type="LongComment" minOccurs="0">
				<xs:annotation>
					<xs:documentation>used to combine significant fields into a single string in order to be able to make automatic re-printing of the document in case of changes in the reservation.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="FilePrefix" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Will be used as macro parameter %FilePrefix% for the DocDelivery.File.FileName element </xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DefaultFileName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Default user-friendly file name to be used in UI prompts and email attachments. Should contain only name portion of the file name (no path or extension). If not set - DocCode will be used for email attachments.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Quantity" type="xs:nonNegativeInteger" default="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>used in case of identical documents, for example, meal coupons.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Params" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Param" type="DocInstanceParameter" minOccurs="0"
							maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SortAttrs" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SortAttr" type="DocInstanceSortAttr" minOccurs="0"
							maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="DocAttachment">
		<xs:annotation>
			<xs:documentation>Individual item in a document group.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Instance" type="DocInstance">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Recipient" type="DocRecipient" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Overridden value of Recipient for the Instance</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--============================================= Description of the requested document-->
