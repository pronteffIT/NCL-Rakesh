										</xs:simpleType>
									</xs:element>
									<xs:element name="LevelStart" type="xs:date"/>
									<xs:element name="LevelEnd" type="xs:date" minOccurs="0"/>
								</xs:all>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Clients" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Output only for Client Program</xs:documentation>
