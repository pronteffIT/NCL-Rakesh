	<xs:complexType name="BorderFormInfo">
		<xs:annotation>
			<xs:documentation>Passenger Info for customs, manifests, etc. It represents data required by border authorities.</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="ResID" type="RecordID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Can be provided to associate passenger information with a specific booking. The calling program should look for such record first if booking number is known. Then it should look for the record without ResID.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information on the identification document.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Type" type="LongCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Document Type, for example, PASSPORT, DRIVER LICENSE, etc.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Document Number</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
									<xs:whiteSpace value="collapse"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="IssuedCountry" type="CountryCode" minOccurs="0"/>
						<xs:element name="IssuedCity" type="CityName" minOccurs="0"/>
						<xs:element name="ExpirationDate" type="xs:date" minOccurs="0"/>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="FormInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information on the form, which was used as source of the passenger information, for example, I-94.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Type" type="Code" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Form Type or Code, for example I-94</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Document Number</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
									<xs:whiteSpace value="collapse"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="IssuedCountry" type="CountryCode" minOccurs="0"/>
						<xs:element name="IssuedCity" type="CityName" minOccurs="0"/>
						<xs:element name="ExpirationDate" type="xs:date" minOccurs="0"/>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="IsCompleted" type="Boolean">
				<xs:annotation>
					<xs:documentation>'N' can be used to store incomplete information in order to request completion at a later time.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Visa" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Visa Information</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Type" type="VisaType" minOccurs="0"/>
						<xs:element name="Number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Visa Number</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
									<xs:whiteSpace value="collapse"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="IssuedCountry" type="CountryCode" minOccurs="0"/>
						<xs:element name="IssuedCity" type="CityName" minOccurs="0"/>
						<xs:element name="IssuedDate" type="xs:date" minOccurs="0"/>
						<xs:element name="ExpirationDate" type="xs:date" minOccurs="0"/>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="EmergencyContact" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Emergency Contact information</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Name" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
									<xs:whiteSpace value="collapse"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="PhoneNumber" type="PhoneNumber" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Phone number</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="IntlCode" minOccurs="0">
							<xs:annotation>
								<xs:documentation>International (country, region) code</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:totalDigits value="5"/>
									<xs:fractionDigits value="0"/>
									<xs:whiteSpace value="collapse"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="Return" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="Airline" type="Airline" minOccurs="0"/>
						<xs:element name="Flight" type="FlightNo" minOccurs="0">
							<xs:annotation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="Date" type="xs:date" minOccurs="0"/>
						<xs:element name="Other" type="Code" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Type of other means of transportation</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="DestinationAddress" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="Street" type="AddressLine" minOccurs="0">
							<xs:annotation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="City" type="CityName" minOccurs="0">
							<xs:annotation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="State" type="StateCode" minOccurs="0">
							<xs:annotation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="Country" type="CountryCode" minOccurs="0">
							<xs:annotation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="ZIP" type="ZipCode" minOccurs="0">
							<xs:annotation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:simpleType name="VisaType">
		<xs:annotation>
			<xs:documentation>Visa Type, for example, H1, H4, etc.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
