<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xhtml="http://www.w3.org/1999/xhtml" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="BaseTypes.xsd"/>
	<xs:annotation>
		<xs:documentation>This schema contains data type definitions for client/household management operations</xs:documentation>
	</xs:annotation>
	<xs:complexType name="Client">
		<xs:annotation>
			<xs:documentation>Client profile.</xs:documentation>
			<xs:appinfo source="Versonix">
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="ClientHeader" type="ClientHeader">
				<xs:annotation>
					<xs:documentation>Basic client data (like first/last name, birthday, nationality, etc)</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Phones" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client phones. Unlimited amount of phones can be specified.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Phone" type="Phone" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Client phone of specific type</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Preferences" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client preferences (like diet, accommodation preferences, etc). Unlimited amount of preferences is allowed but all preference types must be previously set up in SeaWare database.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Preference" type="ClientPreference" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Client preference of specific type</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Classifications" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client classifications - additional user defined properties to classify clients. Unlimited amount of classifications is allowed but all classification types must be previously set up in SeaWare database.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Classification" type="Classification" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Client classification property</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Programs" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client marketing programs (programs that are applied to this client, i.e. client is member of them). Unlimited amount of programs is allowed but all of them must be previously set up in SeaWare database.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Program" type="ProgramMembership" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Client marketing program</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SpecialDates" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Dates that have special meaning for client (like any kind of anniversaries, etc)</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SpecialDate" type="ClientSpecialDate" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Client special date pf specific type</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CarRegistrations" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information about cars that belong to this client. <xhtml:span class="attention">Supported in 041113 only</xhtml:span>
					</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CarRegistration" type="ClientCarRegistration" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Client car details</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BorderForms" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BorderForm" type="BorderFormInfo" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="ClientHeader">
		<xs:annotation>
			<xs:documentation>Common Data for the Client</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="ClientID" type="RecordID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If positive - refers to an existing record in the Seaware client database. If omitted or has zero value -  defines new client profile. If negative - refers to previously created Client (that is already stored in temporary storage)</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="HouseholdID" type="RecordID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Refers to the Household element for the client. The household profile can either be stored in the client database on the cruise line side or be maintained as part of the session. The system will first try to locate the profile in the Household elements of the session and then will go into the client database (only if provided HouseholdID is greater than 0). Negative numbers (-1, -2, etc.) should be used for new households.  <xhtml:span class="versiondiff">In 041113 this is a not mandatory field</xhtml:span>
					</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Last name must be provided for the StoreBooking and StoreClient messages.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FirstName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>First name must be provided for the StoreBooking and StoreClient messages.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MiddleName" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EMail" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Email address</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="80"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DefaultDocDelivery" type="DeliveryType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Default delivery type for travel documents.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsActive" type="Boolean" default="Y" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Inactive clients should not be shown in dropdowns or other selection lists by default. Other than that they should be still functional.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Salutation" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Suffix" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Title" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Occupation" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Birthday" type="xs:date" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client's birthday. If nil="true" than birthday data will be cleared in database.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="BirthPlace" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client's birth place (city, town, etc).</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CountryOfBirth" type="CountryCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client's birth country.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Gender" type="Gender" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client's gender.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Nationality" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client's nationality.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Citizenship" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client's citizenship.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Language" type="LanguageCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client's native (or preferred) language.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastNameNative" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Last name in Native Language.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FirstNameNative" minOccurs="0">
				<xs:annotation>
					<xs:documentation>First name must in Native Language.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MiddleNameNative" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Middle name in Native Language.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SuffixNative" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Suffix in Native Language.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TitleNative" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Title in Native Language.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
						<xs:whiteSpace value="collapse"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="IsHandicapped" type="Boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Is client handicapped</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="MarketingAllowed" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Does client permit to contact his/her because of some marketing campaigns (to send some marketing materials, promotions, etc). If all transports below set to "false" than client doesn't allow to contact him/her.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Mail" type="Boolean" default="N" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Client permits to receive marketing information by mail (in paper form).</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="EMail" type="Boolean" default="N" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Client permits to receive marketing information by e-mail (in electronic form).</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="Phone" type="Boolean" default="N" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Client permits marketing calls over the phone.</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="SMS" type="Boolean" default="N" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Client permits to receive marketing information on his/her mobile phone by SMS.</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="IsSmoker" type="Boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Does client smoke?</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Passport" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Client's passport data</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="Number" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Passport number</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="30"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="IssuedBy" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The organization who issued the passport</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="30"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="IssuedOn" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The date when passport was issued</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="ExpirationDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The date when passport is expired</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="Referral" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The source of information about company's services for this specific client (e.g. where did they found information about this cruise line company or who referred them to this company, etc.). If no child elements are set then the referral information will be removed from database.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Referral type. Must be predefined in SeaWare setup. Possible values could be: "CLIENT" (other client advised to use company's services), "INTERNET" (client has found company somewhere in the Internet), etc</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="30"/>
									<xs:whiteSpace value="collapse"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Date" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="ClientID" type="RecordID" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Reference to an existing client in SeaWare in case if referred by a person.. If nil="true" then client reference will be removed from database.</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="HouseholdID" type="RecordID" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Reference to existing household in SeaWare in case if referred by a household member. If nil="true" then household reference will be removed from database.</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="Source" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Some additional information about referral source for some referral types. For example, if referral type is INTERNET this field can contain address of the site or search engine that was used by client.</xs:documentation>
								<xs:appinfo source="Versonix">
									<XMLBridge>Implemented</XMLBridge>
