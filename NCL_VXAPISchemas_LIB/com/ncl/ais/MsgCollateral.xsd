				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="AddressSummary" type="LongComment" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="CollateralOrderItem">
        <xs:annotation>
            <xs:documentation>Collateral order item.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:all>
            <xs:element name="ItemID" type="RecordID" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="OrderID" type="RecordID" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="CollateralCode" type="CollateralCode" >
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
