    <xs:complexType name="CollateralOrderAltAddress">
        <xs:annotation>
            <xs:documentation>Collateral order alternative address.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:all>
            <xs:element name="OrderID" type="RecordID" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="AltAddressID" type="RecordID" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="Line1" type="AddressLine" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="Line2" type="AddressLine" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="City" type="CityCode" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="State" type="StateCode" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="ZIP" type="ZipCode" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
