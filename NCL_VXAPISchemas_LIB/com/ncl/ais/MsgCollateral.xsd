    <!--=============================================== Collateral Maintenance: Requests -->
    
    <xs:element name="GetListOfCollateralOrders_IN">
        <xs:annotation>
            <xs:documentation>Return list of collateral orders for Requester Type('AGENCY','AGENT','INDIVIDUAL')  and ID.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="MsgHeader" type="MsgHeader">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
                <xs:choice>
                    <xs:element name="ClientID" type="RecordID">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
                    <xs:element name="AgentID" type="RecordID">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
                    <xs:element name="AgencyID" type="RecordID">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
                </xs:choice>
                <xs:element name="DateRange" type="DateTimeRange" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="GetListOfCollateralOrders_OUT">
        <xs:annotation>
            <xs:documentation>Return list of collateral orders.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="MsgHeader" type="MsgHeader">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
                <xs:element name="Orders" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Order" maxOccurs="unbounded" type="CollateralOrder" minOccurs="0">
					<xs:annotation>
						<xs:appinfo source="Versonix">
							<XMLBridge>Implemented</XMLBridge>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Errors" type="Errors" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="GetAvailableCollateralCodes_IN">
        <xs:annotation>
            <xs:documentation>
                If RequesterType is set: Return list of collateral codes eligible for the requester type and current date. if requester type='' then only effective date will be used for search.
                If OrderID is set: Return list of collateral codes eligible for collateral order specified by ID. if Order ID=0 then for new order with default values.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="MsgHeader" type="MsgHeader">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
                <xs:choice minOccurs="0">
                    <xs:element name="OrderID" type="RecordID">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
                    <xs:element name="RequesterType" type="CollateralRequesterType" >
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
                </xs:choice>                
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="GetAvailableCollateralCodes_OUT">
        <xs:annotation>
            <xs:documentation>Response for GetAvailableCollateralCodes_IN request.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="MsgHeader" type="MsgHeader">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
                <xs:element name="AvailableCollateralCodes">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Code" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:all>
                                        <xs:element name="Code" type="CollateralCode">
						<xs:annotation>
							<xs:appinfo source="Versonix">
								<XMLBridge>Implemented</XMLBridge>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
                                        <xs:element name="RequesterType" type="CollateralRequesterType">
						<xs:annotation>
							<xs:appinfo source="Versonix">
								<XMLBridge>Implemented</XMLBridge>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
                                        <xs:element name="DateTimeRange" type="DateTimeRange">
						<xs:annotation>
							<xs:appinfo source="Versonix">
								<XMLBridge>Implemented</XMLBridge>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
                                        <xs:element name="MaxQty" type="Quantity" minOccurs="0">
						<xs:annotation>
							<xs:appinfo source="Versonix">
								<XMLBridge>Implemented</XMLBridge>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
                                        <xs:element name="PerOrder" type="Boolean" minOccurs="0">
						<xs:annotation>
							<xs:appinfo source="Versonix">
								<XMLBridge>Implemented</XMLBridge>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
                                        <xs:element name="Name" type="ShortComment" minOccurs="0">
						<xs:annotation>
							<xs:appinfo source="Versonix">
								<XMLBridge>Implemented</XMLBridge>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
                                        <xs:element name="Comments" type="ShortComment" minOccurs="0">
						<xs:annotation>
							<xs:appinfo source="Versonix">
								<XMLBridge>Implemented</XMLBridge>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
                                    </xs:all>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Errors" type="Errors" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="LoadCollateralOrder_IN">
        <xs:annotation>
            <xs:documentation>Return collateral order data. If Order ID=0 then a new order will be created.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="MsgHeader" type="MsgHeader">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
                <xs:element name="Options" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
