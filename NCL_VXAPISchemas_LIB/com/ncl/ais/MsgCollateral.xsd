<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:include schemaLocation="BaseTypes.xsd"/>
    <xs:include schemaLocation="MsgAccounting.xsd"/>
    <!--=============================================== Collateral Maintenance: basic data types-->
    
    <xs:simpleType name="CollateralOrderStatus">
        <xs:annotation>
            <xs:documentation>Statuses that collateral order may have.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
            <xs:whiteSpace value="collapse"/>
            <xs:enumeration value="FULLFILLED"/>
            <xs:enumeration value="NEW"/>
            <xs:enumeration value="PENDING"/>
            <xs:enumeration value="READY"/>
            <xs:enumeration value="PRINTING"/>
            <xs:enumeration value="CANCELED"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="CollateralAddressType">
        <xs:annotation>
            <xs:documentation>Collateral order address type.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
            <xs:whiteSpace value="collapse"/>
            <xs:enumeration value="DEFAULT"/>
            <xs:enumeration value="ORDER SPECIFIC"/>
            <xs:enumeration value="OTHER"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="CollateralCode">
        <xs:annotation>
            <xs:documentation>Collateral code.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="Code"/>
    </xs:simpleType>

    <xs:simpleType name="CollateralOrderCategory">
        <xs:annotation>
            <xs:documentation>Collateral order category ('COUPON', 'INTERNET', 'CONSUMER', 'BRAND TV', 'PROMOTIONS', 'SMS', etc.).</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="Code"/>
    </xs:simpleType>
    
    <xs:simpleType name="CollateralRequesterType">
        <xs:annotation>
            <xs:documentation>Collateral order requester type.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
            <xs:whiteSpace value="collapse"/>
            <xs:enumeration value="INDIVIDUAL"/>
            <xs:enumeration value="TRAVEL AGENT"/>
            <xs:enumeration value="AGENCY"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="CollateralProviderCode">
        <xs:annotation>
            <xs:documentation>Collateral order provider code.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="Code"/>
    </xs:simpleType>
    
    <xs:simpleType name="CollateralDeliveryCode">
        <xs:annotation>
            <xs:documentation>Collateral order delivery code ('STANDARD', 'OVERNIGHT', 'SPECIAL', etc.).</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="Code"/>
    </xs:simpleType>
    
    <!--=============================================== Collateral Maintenance: collateral data types-->
    
    <xs:complexType name="CollateralOrder">
        <xs:annotation>
            <xs:documentation>Collateral order.</xs:documentation>
		<xs:appinfo source="Versonix">
			<XMLBridge>Implemented</XMLBridge>
		</xs:appinfo>
        </xs:annotation>
        <xs:all>
            <xs:element name="OrderHeader" type="CollateralOrderHeader">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
            </xs:element>
            <xs:element name="OrderItems" minOccurs="0">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Item" minOccurs="0" maxOccurs="unbounded" type="CollateralOrderItem">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
		        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="CollateralOrderAltAddress" type="CollateralOrderAltAddress" minOccurs="0">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
            </xs:element>
        </xs:all>
    </xs:complexType>
    
