					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="OrderStatus" type="CollateralOrderStatus" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="ProviderCode" type="CollateralProviderCode" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="DeliveryCode" type="CollateralDeliveryCode" minOccurs="0">
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>Implemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
		</xs:element>
            <xs:element name="DueDate" type="xs:dateTime" minOccurs="0">
