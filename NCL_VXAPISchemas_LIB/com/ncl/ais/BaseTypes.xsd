<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xhtml="http://www.w3.org/1999/xhtml" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0">
	<xs:annotation>
		<xs:documentation>This schema contains basic data type definitions</xs:documentation>
	</xs:annotation>
	<!--=============================================== Generic-->
	<xs:simpleType name="RecordID">
		<xs:annotation>
			<xs:documentation>Uniquely identifies record in the set of similar records. If positive - refers to an existing record in Seaware database, it negative - refers to (or defines) a record inside the XML document.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:totalDigits value="9"/>
			<xs:fractionDigits value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RecordID64">
		<xs:annotation>
			<xs:documentation>Record ID represented as Int64 internally.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:totalDigits value="18"/>
			<xs:fractionDigits value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RecordIDList">
		<xs:list itemType="xs:long"/>
	</xs:simpleType>
	<xs:simpleType name="RecordID64List">
		<xs:list itemType="RecordID64"/>
	</xs:simpleType>
	<xs:simpleType name="Boolean">
		<xs:annotation>
			<xs:documentation>Boolean: 'Y' or 'N'.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1"/>
			<xs:whiteSpace value="collapse"/>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>Boolean false value</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>Boolean true value</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ShortComment">
		<xs:annotation>
			<xs:documentation>Short comment.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LongComment">
		<xs:annotation>
			<xs:documentation>Long comment.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2000"/>
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LongDescription">
		<xs:annotation>
			<xs:documentation>Full description stored as CLOB in Oracle database</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DayOfWeek">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:whiteSpace value="collapse"/>
			<xs:enumeration value="SUNDAY"/>
			<xs:enumeration value="MONDAY"/>
			<xs:enumeration value="TUESDAY"/>
			<xs:enumeration value="WEDNESDAY"/>
			<xs:enumeration value="THURSDAY"/>
			<xs:enumeration value="FRIDAY"/>
			<xs:enumeration value="SATURDAY"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PasswordResetMode">
		<xs:annotation>
			<xs:documentation>Password reset mode.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:whiteSpace value="collapse"/>
			<xs:enumeration value="SILENT">
				<xs:annotation>
					<xs:documentation>Password is reset in the database, no other action is taken</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RESPONSE">
				<xs:annotation>
					<xs:documentation>New password is returned in the response message</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOTIFY">
				<xs:annotation>
					<xs:documentation>New password is sent via email to the client or agent</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ListOfPwdResetModes">
		<xs:list itemType="PasswordResetMode"/>
	</xs:simpleType>
	<xs:simpleType name="Money">
		<xs:annotation>
			<xs:documentation>Represents monetary data.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="10"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Percent">
		<xs:annotation>
			<xs:documentation>Percent.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="5"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SeqNumber">
		<xs:annotation>
			<xs:documentation>Sequential number.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="5"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Count">
		<xs:annotation>
			<xs:documentation>Count.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:totalDigits value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DateRange">
		<xs:annotation>
			<xs:documentation>Date from/to</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="From" type="xs:date">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="To" type="xs:date">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OpenDateRange">
		<xs:annotation>
			<xs:documentation>Date from/to</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="From" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="To" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DateTimeRange">
		<xs:annotation>
			<xs:documentation>Date/Time from and to</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="From" type="xs:dateTime">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="To" type="xs:dateTime">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OpenDateTimeRange">
		<xs:annotation>
			<xs:documentation>Date from/to</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="From" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="To" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimeRange">
		<xs:annotation>
			<xs:documentation>Time from and to</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="From" type="xs:time">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="To" type="xs:time">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Code">
		<xs:annotation>
			<xs:documentation>Generic code (string(15)).</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LongCode">
		<xs:annotation>
			<xs:documentation>Generic code (string(30)).</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Quantity">
		<xs:annotation>
			<xs:documentation>Quantity, number of units (whole number).</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:totalDigits value="5"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Priority">
		<xs:annotation>
			<xs:documentation>Priority</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:maxInclusive value="999"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DynamicFields">
		<xs:annotation>
			<xs:documentation>Vendor-specific fields (additional attributes) associated with major elements defined in this schema. In other words they are used to allow dynamic extension of the schema.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Field" type="DynamicField" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DynamicField">
		<xs:annotation>
			<xs:documentation>See DynamicFields.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="DynamicFieldName">
				<xs:annotation>
					<xs:documentation>Name of the dynamic field</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:group ref="DynamicFieldValue" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Value of the dynamic field</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:group>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="DynamicFieldName">
		<xs:annotation>
			<xs:documentation>Name of the dynamic field</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="LongCode"/>
	</xs:simpleType>
	<xs:simpleType name="double-or-empty">
		<xs:union memberTypes="xs:double empty-string"/>
	</xs:simpleType>
	<xs:simpleType name="dateTime-or-empty">
		<xs:union memberTypes="xs:dateTime empty-string"/>
	</xs:simpleType>
	<xs:simpleType name="empty-string">
		<xs:restriction base="xs:string">
			<xs:enumeration value=""/>
		</xs:restriction>
	</xs:simpleType>
	<xs:group name="DynamicFieldValue">
		<xs:annotation>
			<xs:documentation>Value of the dynamic field</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:annotation>
				<xs:appinfo source="Versonix">
					<XMLBridge>ToBeImplemented</XMLBridge>
				</xs:appinfo>
			</xs:annotation>
			<xs:element name="StrVal">
				<xs:annotation>
					<xs:documentation>String value</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2000"/>
						<xs:whiteSpace value="preserve"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NumVal" type="double-or-empty">
				<xs:annotation>
					<xs:documentation>Numeric value</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateVal" type="dateTime-or-empty" nillable="true">
				<xs:annotation>
					<xs:documentation>Date and time value</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:group>
	<xs:simpleType name="RowType">
		<xs:annotation>
			<xs:documentation>Generalization of the Table Name concept used in Seaware internal metadata tables. Possible values are defined in the VCS_ROW_TYPE table.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TableName">
		<xs:annotation>
			<xs:documentation>Oracle table name, should be in upper case.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ColumnName">
		<xs:annotation>
			<xs:documentation>Oracle column name, should be in upper case.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CannedCommentType">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
			<xs:enumeration value="ResHistory"/>
			<xs:enumeration value="ResCancel"/>
			<xs:enumeration value="ResCharges"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StateKey">
		<xs:annotation>
			<xs:documentation>Key is used for Send-Pickup suspended sessions</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="128"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<!--=============================================== Translation support-->
	<!--Any simple element in output messages can be marked with attributes from TranslAttrGroup, which is used as an indication for the server to translate value of that element if translation is requested in the input message.-->
	<!--In order for the server to be able to perform translation it needs to know origin of the value of the element - the row type (table name) it belongs too, column name there and the record ID, if possible.-->
	<!--In case of DbSearch such setup should be provided as part of DbSearch definition XML with the exception of Internally Processed nodes. In all other cases it should be provided as annotation of type xs:appinfo to the element in the schema. The annotation should be an instance of the Translation element.-->
	<!--"\Stored Procedures\MetaInfo\TranslationFieldSetup.sql" should be updated too when the Translation setup changes in the schema.-->
	<xs:attributeGroup name="TranslAttrGroup">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:attribute name="Language" type="LanguageCode" use="optional"/>
		<xs:attribute name="Translation" type="LongComment" use="optional"/>
	</xs:attributeGroup>
	<xs:complexType name="TranslatedValue">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attributeGroup ref="TranslAttrGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="RankedTranslatedValue">
		<xs:simpleContent>
			<xs:extension base="TranslatedValue">
				<xs:attribute name="DisplayRank" type="Count"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="Translation">
		<xs:annotation>
			<xs:documentation>To be used in schema annotations to define how translated values should be obtained for given element of an output message (see TranslAttrGroup and examples in MsgDbSearch.xsd and other parts of the schema).</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="TableName" type="TableName">
					<xs:annotation>
						<xs:documentation>Row Type (table name) where source (original) value is stored.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ColumnName" type="ColumnName">
					<xs:annotation>
						<xs:documentation>Name of the column where original value is stored.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice minOccurs="0">
					<xs:element name="RecIDPath" type="XPath">
						<xs:annotation>
							<xs:documentation>XPath to the element containing record ID of the table record. The XPath should be relative to the parent node of the element being translated. If not set translation will be always value-based as opposed to mixed value- and record-based. In record-based translation each record contains translations of its fields to other languages. In value-based translation translated values are determined based on original value, which allows to have less setup if same values are repeated in multiple table records. In case of unsuccessful record-based translation attempt the system performs value-based translation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:sequence>
						<xs:annotation>
							<xs:documentation>As an alternative when record ID is not present in the message, other unique record identifier can be used too. It will be eventually converted to record ID anyway.</xs:documentation>
						</xs:annotation>
						<xs:element name="CodePath" type="XPath">
							<xs:annotation>
								<xs:documentation>XPath to the element with unique record identifier (code).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CodeColumn" type="ColumnName">
							<xs:annotation>
								<xs:documentation>Table (row type) column name corresponding to that code.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--=============================================== Documents related-->
	<xs:simpleType name="DocFileName">
		<xs:annotation>
