	<!---->
	<!--=============================================== Inventory-->
	<xs:simpleType name="InventoryResult">
		<xs:annotation>
			<xs:documentation>Inventory result. OK - a physical inventory slot was allocated (cabin, room, etc.), GTY- physical space is guaranteed, WTL - waitlist, NO - either nothing is available or impossible to book there, for example, NO can be indicated for categories, where all cabins have capacity less than party size.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="Code">
			<xs:enumeration value="OK">
				<xs:annotation>
					<xs:documentation>A physical inventory slot was allocated (cabin, room, etc.).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GTY">
				<xs:annotation>
					<xs:documentation>Physical space is guaranteed according to requested parameters though exact inventory slot is not allocated yet. For example, cabin of requested category is reserved but exact cabin number is not allocated yet.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WTL">
				<xs:annotation>
					<xs:documentation>Allocation is put in the waitlist.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NO">
				<xs:annotation>
					<xs:documentation>Either nothing is available or impossible to book there, for example, NO can be indicated for categories, where all cabins have capacity less than party size.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AvailabilityMode">
		<xs:annotation>
			<xs:documentation>Availability mode. OK - return only packages/categories where cabins are available, GTY- return packages/categories where cabins or guarantees are available, WTL - same as GTY plus packages/categories which have open waitlist, ALL - returns all packages/categories. Default - GTY.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="Code">
			<xs:enumeration value="OK"/>
			<xs:enumeration value="GTY"/>
			<xs:enumeration value="WTL"/>
			<xs:enumeration value="ALL"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AvailabilityResult">
		<xs:annotation>
			<xs:documentation>Availability result. OK - cabins and, possibly, guarantees are available, GTY- only guarantees are available, WTL - only waitlist is open, NO - either nothing is available or impossible to book there, for example, NO can be indicated for categories, where all cabins have capacity less than party size.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="InventoryResult"/>
	</xs:simpleType>
	<xs:simpleType name="Rank">
		<xs:annotation>
			<xs:documentation>Rank. Example: cabin categories are assigned ranks according to their relative expensiveness - lower rank corresponds to more expensive category. Rank values only make sense in relation to each other, i.e. standalone rank value does not mean anything.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="5"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="InventoryUsage">
		<xs:annotation>
			<xs:documentation>Inventory usage totals</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Result" type="AvailabilityResult">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Abs" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Total number of requests with given inventory result in the related queue.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Wgt" type="xs:double">
				<xs:annotation>
					<xs:documentation>Weighted number of requests.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AvailabilityTotals">
		<xs:annotation>
			<xs:documentation>Inventory availability totals.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Abs" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Total number of requests, which can be taken before absolute limits are exhausted. Can be returned as 99999 instead of actual value, which means 'plenty of space', depending on cruise line security settings.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Wgt" type="xs:double">
				<xs:annotation>
					<xs:documentation>Total number of requests, which can be taken before weighted limits are exhausted. Can be returned as 99999 instead of actual value, which means 'plenty of space', depending on cruise line security settings.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="InventoryReqType">
		<xs:annotation>
			<xs:documentation>Inventory request type.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="Code">
			<xs:enumeration value="STANDARD"/>
			<xs:enumeration value="GTY ONLY"/>
			<xs:enumeration value="WTL ONLY"/>
			<xs:enumeration value="NO GTY"/>
			<xs:enumeration value="NO WTL"/>
			<xs:enumeration value="IMPROVEMENT"/>
			<xs:enumeration value="RESERVE"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CabinAssignment">
		<xs:annotation>
			<xs:documentation>Cabin number with date range.</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sail" type="SailRange">
