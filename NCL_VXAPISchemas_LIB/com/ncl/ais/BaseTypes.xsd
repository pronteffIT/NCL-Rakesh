			<xs:enumeration value="RECALL"/>
			<xs:enumeration value="ADJUSTMENT"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AgencyInvoicingNumber">
		<xs:annotation>
			<xs:documentation>Invoicing customer number</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger"/>
	</xs:simpleType>
	<xs:simpleType name="RefNumber">
		<xs:annotation>
			<xs:documentation>Invoice Reference Number</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="InvNumber">
		<xs:annotation>
			<xs:documentation>Invoice Number</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:integer"/>
	</xs:simpleType>
	<xs:simpleType name="InvoiceStatus">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:maxLength value="15"/>
			<xs:enumeration value="ACTIVE"/>
			<xs:enumeration value="INACTIVE"/>
			<xs:enumeration value="CLOSED"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CouponClass">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="25"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TermsType">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
			<xs:whiteSpace value="collapse"/>
			<xs:enumeration value="AUTO"/>
			<xs:enumeration value="MANUAL"/>
			<xs:enumeration value="REQ_CONFIRM"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="InvoiceTotals">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="InvoiceTotal" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total invoice amount in the reservation currency</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="InvoicePaid" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Invoice amount that is already paid</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChargeTotal" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total amount of charges (both automatic and manual)</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ComTotal" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total amount of calculated commissions to agencies</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ComPaid" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Amount of commission that is already paid</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="InsuranceTotal" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total amount of insurance price based on invoice</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="InsurancePaid" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Amount of insurance price that is already paid</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>ToBeImplemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrossDue" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The rest amount to pay by Client</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="NetDue" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The rest amount to pay by Agency</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrentDue" type="CurrentDueType" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CodeValuePair">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="Code" type="LongCode"/>
			<xs:element name="Value" type="xs:string"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="CCTokenInfo">
		<xs:sequence>
			<xs:element name="CCTokenType" type="CCTokenType">
				<xs:annotation>
					<xs:documentation>CC SERVER TYPE that supports the CCToken provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CCToken" type="CCToken">
				<xs:annotation>
					<xs:documentation>Credit Card Token</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CCNumberMasked" type="xs:string" default="XXXXXXxxxxxxXXXX" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Obfuscated Credit Card Number. Used to store the new Card Holder Data</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ExternalDataInfo">
		<xs:annotation>
			<xs:appinfo source="Versonix">
				<XMLBridge>To Be Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="RecordID" type="RecordID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If positive - refers to an existing record in the Seaware  database. If omitted or has zero value -  defines new external data record.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Username" type="Username" minOccurs="0"/>
			<xs:element name="Timestamp" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="Status" type="LongCode" minOccurs="0"/>
			<xs:element name="AppCode" type="ExternalSystemCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Allow to distinguish data for different applications.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MetaData" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Free form metadata in any data representation text format (JSON, XML). External application specific.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Data" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Free form full data representation. External application specific.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Comments" type="ShortComment" minOccurs="0"/>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="Departure">
		<xs:all>
			<xs:element name="Ship" type="ShipCode" minOccurs="0"/>
			<xs:element name="TimePrecision" type="SailTimePrecision" minOccurs="0"/>
			<xs:element name="From" type="SailRef"/>
		</xs:all>
	</xs:complexType>
	<xs:simpleType name="TimeStamp">
		<xs:annotation>
			<xs:documentation>This type should be used when time zone information is explicitly specified.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="SecretCode">
		<xs:annotation>
			<xs:documentation>Describes Guest's security Code.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Code"/>
	</xs:simpleType>
	<xs:complexType name="ExtItem">
		<xs:sequence>
			<xs:element name="Key" type="ExtKey">
				<xs:annotation>
					<xs:documentation>Key of the data that should be used as unique identifier of this data</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Data" type="ExtData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If omitted - the Key will be removed from the list.</xs:documentation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CurrentDueType">
		<xs:sequence>
			<xs:element name="DueDate" type="xs:date"/>
			<xs:element name="AmountDue" type="Money">
				<xs:annotation>
					<xs:appinfo source="Versonix">
						<XMLBridge>Implemented</XMLBridge>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="DueType" type="DueType">
					<xs:annotation>
						<xs:appinfo source="Versonix">
							<XMLBridge>Implemented</XMLBridge>
						</xs:appinfo>
						<xs:documentation>Payment Schedule Due Item</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ActionCode" type="LongCode">
					<xs:annotation>
						<xs:documentation>Examples: 'FULL CANCELLATION', 'GUEST CANCELLATION', 'AIR DEVIATION', etc.</xs:documentation>
						<xs:appinfo source="Versonix">
							<XMLBridge>Implemented</XMLBridge>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="ShorexType" type="LongCode"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SailActivityExtended">
		<xs:all>
			<xs:element name="SailActivityID" type="RecordID"/>
			<xs:element name="SailRefID" type="RecordID" minOccurs="0"/>
			<xs:element name="Ship" type="ShipCode"/>
			<xs:element name="ActivityType" type="SailActivityType"/>
			<xs:element name="DateTime" type="xs:dateTime"/>
			<xs:element name="MayEmbark" type="Boolean"/>
			<xs:element name="MayDisembark" type="Boolean"/>
			<xs:element name="Port" type="PortCode" minOccurs="0"/>
			<xs:element name="Pier" type="PierCode" minOccurs="0"/>
			<xs:element name="Description" type="ShortComment" minOccurs="0"/>
			<xs:element name="Notes" type="LongComment" minOccurs="0"/>
			<xs:element name="Ownership" type="OwnershipCode" minOccurs="0"/>
			<xs:element name="DynamicFields" type="DynamicFields" minOccurs="0"/>
			<xs:element name="CargoStatus" type="DepartureStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Departure status for cargo check-in.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaxStatus" type="DepartureStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Departure status for passenger check-in.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureStatusHistory" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name should be included into
						Options.IncludeElements
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="HistItem" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:all>
									<xs:element name="Operator" type="OracleUsername"/>
									<xs:element name="TimeStamp" type="xs:dateTime"/>
									<xs:element name="Version" type="xs:integer"/>
									<xs:element name="CargoStatus" type="DepartureStatus" minOccurs="0"/>
									<xs:element name="PaxStatus" type="DepartureStatus" minOccurs="0"/>
									<xs:element name="Comments" type="ShortComment" minOccurs="0"/>
								</xs:all>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Comments" type="ShortComment" minOccurs="0"/>
			<xs:element name="Arrival" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Populated only for "departures" - items with
						SailRefID != 0 and Port != '' and MayEmbark = Y. Contains
						information from the next "arrival" record in the ship
						itinerary.
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:all>
						<xs:element name="SailActivityID" type="RecordID"/>
						<xs:element name="SailRefID" type="RecordID" minOccurs="0"/>
						<xs:element name="DateTime" type="xs:dateTime"/>
						<xs:element name="ActivityType" type="SailActivityType"/>
						<xs:element name="Port" type="PortCode"/>
						<xs:element name="Pier" type="PierCode" minOccurs="0"/>
					</xs:all>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:complexType>
</xs:schema>
