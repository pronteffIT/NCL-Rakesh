<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xhtml="http://www.w3.org/1999/xhtml" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="BaseTypes.xsd"/>
	<xs:include schemaLocation="Agency.xsd"/>
	<xs:include schemaLocation="Client.xsd"/>
	<xs:include schemaLocation="ResShell.xsd"/>
	<xs:include schemaLocation="GroupShell.xsd"/>
	<xs:group name="CreditCardInfoOrRefNum">
		<xs:choice>
			<xs:element name="CCData" type="CreditCard" minOccurs="0"/>
			<xs:element name="CCTransID" type="RecordID" minOccurs="0"/>
		</xs:choice>
	</xs:group>
	<xs:simpleType name="TcType">
		<xs:annotation>
			<xs:documentation>User defineable tour credit type</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TcCalcType">
		<xs:annotation>
			<xs:documentation>Tour Credit Header type</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:maxLength value="15"/>
			<xs:enumeration value="COLLECTIONS"/>
			<xs:enumeration value="FITS"/>
			<xs:enumeration value="GROUP"/>
			<xs:enumeration value="SAIL"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TCDistrType">
		<xs:annotation>
			<xs:documentation>Tour Credit Distribution type</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
			<xs:whiteSpace value="collapse"/>
			<xs:enumeration value="TRANSACTION"/>
			<xs:enumeration value="COMMISSION"/>
			<xs:enumeration value="DISCOUNT"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="TCShell">
		<xs:annotation>
			<xs:documentation>Tour Credit shell</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:all>
			<xs:element name="TCHeader" type="TCHeader"/>
			<xs:element name="CalcDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CalcDetail" type="TCCalcDetail" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Distributions" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Distribution" type="TCDistrDetail" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="TCHeader">
		<xs:annotation>
			<xs:documentation>Tour Credit Header</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TCID" type="RecordID"/>
			<xs:element name="TCType" type="TcType"/>
			<xs:element name="CalcType" type="TcCalcType"/>
			<xs:choice>
				<xs:sequence>
					<xs:annotation>
						<xs:documentation>for Header Type = FITS or SAIL</xs:documentation>
					</xs:annotation>
					<xs:element name="AgencyID" type="RecordID"/>
					<xs:element name="Sail" type="SailRange"/>
					<xs:element name="CollectionID" type="RecordID" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Used in 040808 only</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:element name="GroupID" type="RecordID">
					<xs:annotation>
						<xs:documentation>for Header Type = GROUP</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="IsValid" type="Boolean" minOccurs="0"/>
			<xs:element name="Currency" type="CurrencyCode"/>
			<xs:element name="QtyEarned" type="xs:double"/>
			<xs:element name="QtyQualified" type="xs:double"/>
			<xs:element name="QtyTC" type="xs:double"/>
			<xs:element name="TCPrice" type="Money"/>
			<xs:element name="TCPolicy" type="xs:nonNegativeInteger">
				<xs:annotation>
					<xs:documentation>Calculated value of TC policy</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TCPolicyOvr" type="xs:nonNegativeInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used in 040808 only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CabinCategory" type="CabinCategoryCode" minOccurs="0"/>
			<xs:element name="EarnedCabinCategory" type="CabinCategoryCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used in 040808 only for NCL</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CalcAsFlatAmount" type="Boolean"/>
			<xs:element name="CalcAmount" type="Money"/>
			<xs:element name="AdjAmount" type="Money"/>
			<xs:element name="DistrAmount" type="Money"/>
			<xs:element name="Comments" type="ShortComment" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TCCalcDetail">
		<xs:annotation>
			<xs:documentation>Tour Credit calculation detaiul</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="GroupID" type="RecordID"/>
				<xs:element name="ResID" type="RecordID"/>
			</xs:choice>
			<xs:element name="QtyQualified" type="xs:double"/>
			<xs:element name="QtyTC" type="xs:double"/>
			<xs:element name="SumEarned" type="Money"/>
			<xs:element name="CabinCategory" type="CabinCategoryCode"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TCDistrDetail">
		<xs:annotation>
			<xs:documentation>Tour Credit Distribution item</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>ToBeImplemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="GroupID" type="RecordID"/>
				<xs:element name="ResID" type="RecordID"/>
				<xs:element name="AgencyID" type="RecordID"/>
				<xs:element name="AgencyBankID" type="RecordID"/>
				<xs:element name="AmenityID" type="RecordID"/>
			</xs:choice>
			<xs:element name="DistrType" type="TCDistrType"/>
			<xs:element name="ReqAmount" type="Money">
				<xs:annotation>
					<xs:documentation>Requested amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ApplAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Applied amount (output only)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GuestID" type="RecordID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provided for distribution as discount or commission</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommissCode" type="CommissionCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provided for distribution as commission</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransType" type="Code" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provided for distribution as transaction</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransID" type="RecordID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Refers to generated Transaction for the corresponding distribution type (output only)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="User" type="Username" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Creator of the record. Output only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Timestamp" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Timestamp when the record was created. Output only</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccBatchHeader">
		<xs:all>
			<xs:element name="BatchID" type="RecordID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Output only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsActive" type="Boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Changing form Y to N causes posting the batch if possible</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OpenAt" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Output only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClosedAt" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Output only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FormOfTrans" type="FormOfTrans" minOccurs="0"/>
			<xs:element name="BatchAmount" type="Money" minOccurs="0"/>
			<xs:element name="TransAmount" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Transactions amounts total (output only)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="CurrencyCode" minOccurs="0"/>
			<xs:element name="Comments" type="ShortComment" minOccurs="0"/>
			<xs:element name="User" type="Username" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Output only</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceCode" type="SourceCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Output only</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!--=============================================== Accounting Support-->
	<xs:element name="ManageAccTrans_IN">
		<xs:annotation>
			<xs:documentation>Create/accept/void transactions</xs:documentation>
			<xs:appinfo source="Versonix">
				<XMLBridge>Implemented</XMLBridge>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="MsgHeader" type="MsgHeader">
					<xs:annotation>
						<xs:appinfo source="Versonix">
							<XMLBridge>Implemented</XMLBridge>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="Options" minOccurs="0">
					<xs:annotation>
						<xs:documentation>CC processing parameters</xs:documentation>
						<xs:appinfo source="Versonix">
							<XMLBridge>Implemented</XMLBridge>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="StatusPollQty" type="xs:nonNegativeInteger">
								<xs:annotation>
									<xs:documentation>0 means no waiting</xs:documentation>
									<xs:appinfo source="Versonix">
										<XMLBridge>Implemented</XMLBridge>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
							<xs:element name="StatusPollFreq" type="xs:nonNegativeInteger" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Interval in seconds between pollings</xs:documentation>
									<xs:appinfo source="Versonix">
										<XMLBridge>Implemented</XMLBridge>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Create" minOccurs="0">
					<xs:annotation>
						<xs:appinfo source="Versonix">
							<XMLBridge>Implemented</XMLBridge>
						</xs:appinfo>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="Payments" minOccurs="0">
								<xs:annotation>
									<xs:appinfo source="Versonix">
										<XMLBridge>Implemented</XMLBridge>
									</xs:appinfo>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Amount" type="Money" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Total Amount of all payments</xs:documentation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="Currency" type="CurrencyCode" minOccurs="0">
											<xs:annotation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="Payment" maxOccurs="unbounded">
											<xs:annotation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SrcEntity" type="AccSourcePmntEntity">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="DestEntity" type="AccDestPmntEntity">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="Amount" type="Money">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="Currency" type="CurrencyCode" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="FormOfTrans" type="FormOfTrans">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="TransType" type="Code" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="TransFormInfo" type="TransFormInfo" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="PurchaseInsurance" type="Boolean" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Y indicates that when the payment is being processed, the payment amount includes purchasing Insurance. </xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="ExternalIdent" type="ExternalIdent" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="CCMerchant" type="CCMerchant" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Merchant who creates the CC transaction</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="Comments" type="ShortComment" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Refunds" minOccurs="0">
								<xs:annotation>
									<xs:appinfo source="Versonix">
										<XMLBridge>Implemented</XMLBridge>
									</xs:appinfo>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Amount" type="Money" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Total Amount of all refunds</xs:documentation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="Currency" type="CurrencyCode" minOccurs="0">
											<xs:annotation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="Refund" maxOccurs="unbounded">
											<xs:annotation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SrcEntity" type="AccDestPmntEntity">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="DestEntity">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
														<xs:complexType>
															<xs:choice>
																<xs:element name="ClientID" type="RecordID">
																	<xs:annotation>
																		<xs:appinfo source="Versonix">
																			<XMLBridge>Implemented</XMLBridge>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="AgencyID" type="RecordID">
																	<xs:annotation>
																		<xs:appinfo source="Versonix">
																			<XMLBridge>Implemented</XMLBridge>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
																<xs:element name="GLRefID" type="RecordID">
																	<xs:annotation>
																		<xs:appinfo source="Versonix">
																			<XMLBridge>Implemented</XMLBridge>
																		</xs:appinfo>
																	</xs:annotation>
																</xs:element>
															</xs:choice>
														</xs:complexType>
													</xs:element>
													<xs:element name="TransID" type="RecordID"/>
													<xs:element name="Amount" type="Money">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="Currency" type="CurrencyCode" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="FormOfTrans" type="FormOfTrans">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="TransType" type="Code" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="TransFormInfo" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
														<xs:complexType>
															<xs:group ref="CreditCardInfoOrRefNum"/>
														</xs:complexType>
													</xs:element>
													<xs:element name="ExternalIdent" type="ExternalIdent" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
													<xs:element name="Comments" type="ShortComment" minOccurs="0">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Transfers" minOccurs="0">
								<xs:annotation>
									<xs:appinfo source="Versonix">
										<XMLBridge>Implemented</XMLBridge>
									</xs:appinfo>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Amount" type="Money" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Total Amount of all transfers</xs:documentation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="Currency" type="CurrencyCode" minOccurs="0">
											<xs:annotation>
												<xs:documentation>If provided, shoul be the same as source booking's currency</xs:documentation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="Transfer" maxOccurs="unbounded">
											<xs:annotation>
												<xs:appinfo source="Versonix">
													<XMLBridge>Implemented</XMLBridge>
												</xs:appinfo>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SrcEntity">
														<xs:annotation>
															<xs:appinfo source="Versonix">
																<XMLBridge>Implemented</XMLBridge>
															</xs:appinfo>
														</xs:annotation>
														<xs:complexType>
