	<xs:element name="AutoFinalPayment_IN">
		<xs:annotation>
			<xs:documentation>Credit Card Auto Final Payment input message. Implemented in 040808 View only</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="MsgHeader" type="MsgHeader"/>
				<xs:element name="ResID" type="RecordID">
					<xs:annotation>
						<xs:documentation>Only ResID is supported as AFP Destination entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ClientID" type="RecordID">
					<xs:annotation>
						<xs:documentation>Only ClientID is supported as AFP Source Entity</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AFPOption" type="AFPOption"/>
				<xs:element name="Amount" type="Money" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Final Payment Amount. Must be equal to "GROSS DUE" on Booking at the time of Auto Payment </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CreditCard" minOccurs="0">
					<xs:complexType>
						<xs:choice>
							<xs:element name="CCClientID" type="RecordID">
								<xs:annotation>
									<xs:documentation>Reference to Credit Card (CC_CLIENT table)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="CCData" type="CreditCardBaseInfo"/>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="AutoFinalPayment_OUT">
		<xs:annotation>
			<xs:documentation>Credit Card Auto Final Payment output message. Implemented in 040808 View only</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MsgHeader" type="MsgHeader"/>
				<xs:element name="Status" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Returns Status of AFP record for VALIDATE option:  'N/A'-No setup for booking,  'No AFP Amount Setup'-AFP Amount is not defined,  'DONE'-AFP transaction has been made successfully,  'NOTHING TO PAY'- Amount due has been payed without AFP,   'FUTURE'-AFP Transaction have to be created in future, 'SCHEDULED'-AFP Transaction have to be created in less than 7 days, 'CURRENT' - AFP Transaction missing but Grace Period is not expired yet, 'PAST DUE' -AFP Transaction missing and Grace periond expired</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Errors" type="Errors" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Validation Messages</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
